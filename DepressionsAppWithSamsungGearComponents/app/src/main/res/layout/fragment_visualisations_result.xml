<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.VisualisationsResultFragment">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fadeScrollbars="false"
        android:id="@+id/scrollView" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hier kannst du dir die Ergebnisse deiner Aufzeichnungen ansehen. "
                android:id="@+id/textView53"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="2dip"
                android:background="#9c9c9c"
                android:layout_below="@+id/textView53"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/hyphen1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Stimmung"
                android:id="@+id/textView59"
                android:layout_below="@+id/hyphen1"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Deine Stimmung war in der letzten Woche"
                android:id="@+id/textView60"
                android:layout_below="@+id/textView59"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="schlecht"
                android:gravity="center"
                android:id="@+id/textView61"
                android:layout_below="@+id/textView60"
                android:textColor="@color/Blue"
                android:layout_marginBottom="10dp"
                android:layout_gravity="left"
                android:layout_marginLeft="10dp" />


            <View
                android:layout_width="fill_parent"
                android:layout_height="2dip"
                android:background="#9c9c9c"
                android:layout_below="@+id/textView61"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/hyphen2"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Schlaf"
                android:id="@+id/textView62"
                android:layout_below="@+id/hyphen2"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Du hast in der letzten Woche"
                android:id="@+id/textView63"
                android:layout_below="@+id/textView62"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="durchschnittlich 11h pro Nacht"
                android:gravity="center"
                android:id="@+id/textView64"
                android:layout_below="@+id/textView63"
                android:textColor="@color/Blue"
                android:layout_gravity="left"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="geschlafen"
                android:id="@+id/textView65"
                android:layout_below="@+id/textView64"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="2dip"
                android:background="#9c9c9c"
                android:layout_below="@+id/textView65"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/hyphen3"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Zustand Wohnung"
                android:id="@+id/textView66"
                android:layout_below="@+id/hyphen3"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Deine Wohnung war insgesamt in einem "
                android:id="@+id/textView67"
                android:layout_below="@+id/textView66"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="sehr guten Zustand"
                android:gravity="center"
                android:id="@+id/textView68"
                android:layout_below="@+id/textView67"
                android:textColor="@color/Blue"
                android:layout_gravity="left"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="(Temperatur und Luftqualität im Verlauf der Woche)"
                android:id="@+id/textView69"
                android:layout_below="@+id/textView68"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="10dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="2dip"
                android:background="#9c9c9c"
                android:layout_below="@+id/textView69"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:id="@+id/hyphen4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Fazit"
                android:id="@+id/textView70"
                android:layout_below="@+id/hyphen3"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="10dp"
                android:layout_gravity="left" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Hier evtl eine Art Fazit/Schlussfolgerung/Verbesserungsmöglichkeit?"
                android:id="@+id/textView71"
                android:layout_below="@+id/textView70"
                android:layout_marginLeft="10dp" />

        </LinearLayout>
    </ScrollView>
</FrameLayout>
